{
  "name": "resource-search",
  "version": "1.0.0",
  "description": "A database of resources searchable by tag.",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "dev": "webpack-dev-server --open --config webpack.config.js",
    "format": "prettier --use-tabs --trailing-comma es5 --single-quote --print-width 120  --jsx-single-quote  --write \"{src/**/*.js,src/templates/index.html}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vincenavarro/resource-search.git"
  },
  "keywords": [
